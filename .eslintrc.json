{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {        
        "no-console": "off",
        "no-underscore-dangle":  ["error", { "allow": ["_notes","_service","_validator","_pool"] }],
        "camelcase": "off"
    }
}
